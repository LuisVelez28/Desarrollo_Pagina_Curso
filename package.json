{
  "name": "desarrollo_pagina_curso",
  "version": "1.0.0",
  "description": "Muestra de pagina web en Angular",
  "homepage": "https://github.com/LuisVelez28/Desarrollo_Pagina_Curso#readme",
  "bugs": {
    "url": "https://github.com/LuisVelez28/Desarrollo_Pagina_Curso/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisVelez28/Desarrollo_Pagina_Curso.git"
  },
  "license": "ISC",
  "author": "Luis Felipe Velez Garcia",
  "type": "commonjs",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "less": "lessc css/styles2.less css/styles2.css",
    "less:watch": "onchange css/styles2.less -i -- npm run less",
    "scss": "sass css/styles.scss css/styles.css",
    "scss:watch": "onchange css/styles.scss -i -- npm run scss",
    "start": "concurrently \"npm run less:watch\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/open-iconic/font/fonts/* dist/fonts",
    "copyjs": "copyfiles -f node_modules/jquery/dist/jquery.min.js node_modules/popper.js/dist/umd/popper.min.js node_modules/bootstrap/dist/js/bootstrap.min.js dist/js",
    "imagemin": "imagemin images/* --out-dir=dist/images",
    "usemin": "usemin index.html -d dist -o dist/index.html && usemin hoteles.html -d dist -o dist/hoteles.html && usemin nosotros.html -d dist -o dist/nosotros.html && usemin contacto.html -d dist -o dist/contacto.html && usemin precios.html -d dist -o dist/precios.html",
    "build": "npm run clean && npm run less && npm run copyfonts && npm run copyjs && npm run usemin"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "jquery": "^4.0.0",
    "lite-server": "^2.6.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "less": "^4.5.1",
    "onchange": "^7.1.0",
    "rimraf": "^6.1.3",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
